<div class="main-navigationbar">
    <div class="container">
        <div class="navigationbar-row d-flex align-items-center justify-content-between">
            <div class="menu-items-col">
                <ul class="main-nav">
                    {% assign count = 0 %}
                    {% for block in section.blocks %}
                        {% assign count = count | plus: 1 %}
                        {% assign canonical_url_trim = canonical_url | replace: request.origin %}
                        {% assign block_url_trim = block.settings.link | replace: request.origin %}
                        {% if canonical_url_trim == "/" %}{% assign canonical_url_trim = "" %}{% endif %}
                        {% if block.type == "link-simple" or block.type == "link-linklist" or block.type == "link-4linklists" %}  
                            <li class="menu-lnk{% case block.type %}{% when 'link-linklist' %} has-item{% when 'link-4linklists' %} has-item{% endcase %}">
                                <a class="{{ block.settings.diffclassname }} {% if count == 1 %}category-btn active{% endif %}" href="{% if block.settings.link != blank  %}{{ block.settings.link }} {% else %} javascript:; {% endif %}">
                                    {%  if block.settings.icon != blank %}
                                      <span class="link-icon">
                                        <img src="{{ block.settings.icon | img_url: 'master' }}" alt="icon">
                                      </span>  
                                    {% endif %}
                                    {{ block.settings.title }}
                                </a> 
                                {% case block.type %}
                                    {% when 'link-linklist' %}
                                    {% if block.settings.links.size > 0 %}
                                    <div class="menu-dropdown">
                                        {%- assign links_small = block.settings.links -%}
                                        <ul id="menu_{{ block.id }}">
                                            {% for link in linklists[links_small].links %}
                                                {%- assign child_list_handle = link.title | handleize -%}
                                                {%- assign link_count = 0 -%}
                                                {% for childlink in linklists[child_list_handle].links %}
                                                    {%- assign link_count = link_count | plus: 1 -%}
                                                {% endfor %}
                                                <li class="{% if link_count > 0 %}with_ul{% endif %}">
                                                    <a href="{{ link.url }}">{{ link.title }}{% if link_count > 0 %}<i class="level_3__trigger megamainhd" data-submenu="submenu_{{ block.id }}-{{ link_count }}"></i>{% endif %}</a>
                                                    {% if link_count > 0 %}
                                                        <ul id="1submenu_{{ block.id }}-{{ link_count }}">
                                                            {% for childlink in linklists[child_list_handle].links %}
                                                                {%- assign child_list_2_handle = childlink.title | handleize -%}
                                                                {%- assign link_count = 0 -%}
                                                                {% for childlink in linklists[child_list_2_handle].links %}
                                                                {%- assign link_count = link_count | plus: 1 -%}
                                                                {% endfor %}                                                                        
                                                                <li class="{% if link_count > 2 %}with_ul2{% endif %}">
                                                                    <a href="{{ childlink.url }}">{{ childlink.title }}{% if link_count > 2 %}<i class="level_3__trigger megamainhd" data-submenu="subsubmenu_{{ block.id }}-{{ link_count }}"></i>{% endif %}</a>
                                                                    {% if link_count > 2 %}
                                                                        <ul id="2subsubmenu_{{ block.id }}-{{ link_count }}">
                                                                        {% for childlink in linklists[child_list_2_handle].links %}
                                                                            <li>
                                                                            <a href="{{ childlink.url }}">{{ childlink.title }}</a>
                                                                            </li>
                                                                        {% endfor %}
                                                                        </ul>
                                                                    {% endif %}
                                                                </li>
                                                            {% endfor %}
                                                        </ul>
                                                    {% endif %}
                                                </li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                    {% endif %}
                
                                    {% when 'link-4linklists' %}
                                    {% if block.settings.links_1.size > 0 or block.settings.links_2.size > 0 or block.settings.links_3.size > 0 or block.settings.links_4.size > 0 %}
                                        <div class="mega-menu menu-dropdown">
                                            <div class="mega-menu-container container">
                                                <ul class="row" id="menu_{{ block.id }}">
                                                    {% for i in (1..3) %}  
                                                    {% capture list %}links_{{ i }}{% endcapture %}
                                                    {%- assign list = block.settings[list] -%}
                                                    {% capture listtitle %}link_title{{ i }}{% endcapture %}
                                                    {%- assign listtitle = block.settings[listtitle] -%}
                                                        <li class="col-md-3 col-12">                                          
                                                            <ul class="megamenu-list arrow-list" id="menu_{{ block.id }}-{{ i }}">
                                                                <li class="list-title"><span>{{ listtitle }}</span></li>
                                                                {% for link in linklists[list].links %}
                                                                    <li>
                                                                    {% if link.title contains '[new]' %}
                                                                    {%- assign link_text = link.title | replace: '[new]', '' -%}
                                                                    {%- assign link_badge = 'new' -%}
                                                                    {% elsif link.title contains '[hot]' %} 
                                                                    {%- assign link_text = link.title | replace: '[hot]', '' -%}
                                                                    {%- assign link_badge = 'hot' -%}   
                                                                    {% else %}
                                                                    {%- assign link_text = link.title -%}
                                                                    {%- assign link_badge = false -%}   
                                                                    {% endif %}
                                                                    <a href="{{ link.url }}">{{ link_text }}{% if link_badge %}<span class="badge badge_{{ link_badge }}">{{ link_badge }}</span>{% endif %}</a>
                                                                    </li>
                                                                {% endfor %}
                                                            </ul>
                                                        </li>
                                                    {% endfor %}
                                                    {% if block.settings.image != blank %}
                                                        <li class="col-md-3 col-12">
                                                            <a href="{{ block.settings.imageblink }}"> 
                                                                <img loading="lazy" src="{{ block.settings.image | img_url: 'master' }}" alt="menubaner">
                                                            </a>
                                                        </li>
                                                    {% endif %}
                                                </ul>
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endcase %}
                            </li>
                        {% endif %}
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
</div>
