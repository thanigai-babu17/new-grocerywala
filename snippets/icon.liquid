{%- capture icon_class -%}
icon icon--{{ icon }} {% if inline %}icon--inline{% endif %} {% if direction_aware %}icon--direction-aware{% endif %} {{ class }}
{%- endcapture -%}

{%- case icon -%}
{%- when 'nav-arrow-left' -%}
  <svg focusable="false" width="{{ width | default: 17 }}" height="{{ height | default: 14 }}" class="{{ icon_class }}" viewBox="0 0 17 14">
    <path d="M17 7H2M8 1L2 7l6 6" stroke="currentColor" fill="none"></path>
  </svg>

{%- when 'nav-arrow-right' -%}
  <svg focusable="false" width="{{ width | default: 17 }}" height="{{ height | default: 14 }}" class="{{ icon_class }}" viewBox="0 0 17 14">
    <path d="M0 7h15M9 1l6 6-6 6" stroke="currentColor" fill="none"></path>
  </svg>

{%- when 'nav-arrow-down' -%}
